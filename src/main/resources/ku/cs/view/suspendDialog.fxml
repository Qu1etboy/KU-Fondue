<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" minHeight="-Infinity" minWidth="-Infinity" spacing="20.0" stylesheets="@../css/login.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.controllers.SuspendDialogController">
   <children>
      <Label styleClass="title" text="คุณถูกระงับสิทธิ์การใช้งาน" textFill="RED" />
      <HBox alignment="TOP_CENTER" spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#handleRequestButton" styleClass="login-btn" text="ขอคืนสิทธิ์การใช้งาน" />
            <Button mnemonicParsing="false" onAction="#handleCloseButton" styleClass="login-btn" text="Ok" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
