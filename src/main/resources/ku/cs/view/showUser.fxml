<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.controllers.ShowUserDetailController">
   <children>
      <HBox>
         <children>
            <Button mnemonicParsing="false" onAction="#handleBackButton" text="Back" />
         </children>
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
      </HBox>
      <TableView fx:id="userTable" prefHeight="326.0" prefWidth="516.0">
         <columns>
            <TableColumn fx:id="name" prefWidth="156.0" text="Username" />
            <TableColumn fx:id="role" prefWidth="122.4000244140625" text="role" />
            <TableColumn fx:id="status" prefWidth="220.7999267578125" text="Last Online" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
