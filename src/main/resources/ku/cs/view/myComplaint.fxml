<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.controllers.ComplaintDetailController">
   <children>
      <HBox>
         <children>
            <Button mnemonicParsing="false" onAction="#handleBackButton" text="Back">
               <HBox.margin>
                  <Insets bottom="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <TableView>
         <columns>
            <TableColumn prefWidth="75.0" text="ลำดับ" />
            <TableColumn prefWidth="165.0" text="หมวดหมู่" />
            <TableColumn prefWidth="257.0" text="รายละเอียด" />
            <TableColumn prefWidth="172.0" text="สถานะ" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
