<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.controllers.ComplaintInfoController">
   <children>
      <ScrollPane fitToWidth="true" prefHeight="400.0" prefWidth="600.0" VBox.vgrow="ALWAYS">
         <content>
            <VBox spacing="10.0">
               <children>
                  <HBox>
                     <children>
                        <Button mnemonicParsing="false" onAction="#handleBackButton" text="Back" />
                     </children>
                  </HBox>
                  <VBox fx:id="contentContainer" />
                  <HBox alignment="CENTER_LEFT" spacing="10.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#handleVoteButton" text="โหวต" />
                        <Label fx:id="voteLabel" text="0" />
                        <Button mnemonicParsing="false" text="รายงาน" />
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
         </content>
      </ScrollPane>
   </children>
</VBox>
